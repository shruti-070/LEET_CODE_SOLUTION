class Solution 
{
    public boolean squareIsWhite(String coordinates) 
    {
        char c = coordinates.charAt(0);
        int a = Integer.valueOf(c);
        int b = coordinates.charAt(1);
        if((a%2==0 && b%2!=0) || (a%2!=0 && b%2==0))
            return true;
        else
            return false;
   }
}
